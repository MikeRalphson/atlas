<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.1//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite3.1.dtd">

<urlrewrite>

    <rule>
        <from>^/static/(js|images|css)/(.*)-v([0-9]+).(css|js|png)$</from>
        <to>/static/$1/$2.$4</to>
    </rule>

</urlrewrite>
